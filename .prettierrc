{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "printWidth": 100,
  "arrowParens": "always",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "proseWrap": "preserve",
  "jsxSingleQuote": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "endOfLine": "lf"
}
